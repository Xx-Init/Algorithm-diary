//<head>

//前缀函数  字符串周期
// 周期为len-pi[len-1];

vector<int> prefix_function(string s){ //前缀函数
    int len = s.size();
    vector<int> pi(len);
    for(int i = 1; i < len ; i ++){
        int j = pi[i-1];
        while(j && s[i] != s[j]) j = pi[j-1];
        if(s[i] == s[j]) j ++;
        pi[i] = j;
    }
    return pi;
}

void solve(){
    int len;
    string s1;
    cin>> len>> s1;
    vector<int> vt1 = prefix_function(s1);
    cout<< len-vt1[len-1]<< endl;
}

int main(){
    ios::sync_with_stdio(false);
    cin.tie(nullptr);   
    solve();
    return 0;
}
